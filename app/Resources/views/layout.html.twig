<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <title>{% block title %}{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="{% block description %}{% endblock %}">
        <meta name="author" content="Android-dev">
        <link rel="canonical" href="{% block urlCanonical %}{% endblock %}" />
        <link rel="alternate" type="application/rss+xml" href="{{ path('androiddev_feed1') }}" title="Derniers articles">
        <link rel="alternate" type="application/rss+xml" href="{{ path('androiddev_feed2') }}" title="Dernieres astuces">
        <link rel="alternate" type="application/rss+xml" href="{{ path('androiddev_feed3') }}" title="Derniers articles et astuces">
        <link rel="alternate" type="application/rss+xml" href="{{ path('androiddev_feed4') }}" title="Derniers projets">
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('img/fav.ico') }}" />

        <script type="text/javascript">
            (function() {
                var po = document.createElement("script");
                po.type = "text/javascript";
                po.async = true;
                po.src = "https://apis.google.com/js/plusone.js?publisherid=109789572578192575458";
                var s = document.getElementsByTagName("script")[0];
                s.parentNode.insertBefore(po, s);
            })();
        </script>

        <!-- Le styles -->
        {% block stylesheets %}

            {% endblock %}

    </head>

    <body>

                        {% block navbar %}

                        {% endblock %}


            {% block infoMobileTablet %}
            {% endblock %}


        <div class="row">
            <div class="col-md-2 hidden-sm hidden-xs">
                    {% block menuGauche %}
                    {% endblock %}
            </div>

            <div class="col-md-7 content col-sm-9" itemscope itemtype="http://schema.org/Article">
                <style>
                    .responsive { width: 320px; height: 50px; }
                    @media(min-width: 500px) { .responsive { width: 468px; height: 60px; } }
                    @media(min-width: 1300px) { .responsive { width: 728px; height: 90px; } }
                </style>
                <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                <!-- responsive -->
                <ins class="adsbygoogle responsive"
                     style="display:inline-block"
                     data-ad-client="ca-pub-4672745701309961"
                     data-ad-slot="8079289372"></ins>
                <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
                </script>

                {% block enteteContentPC %}
                {% endblock %}

                {% block contentPC %}
                 {% endblock %}
                <script type="text/javascript">
                    google_ad_client = "ca-pub-4672745701309961";
                    width = document.documentElement.clientWidth;
                    if (width < 500) {
                        google_ad_slot = "1978297378";
                        google_ad_width = 320;
                        google_ad_height = 50;
                    }
                    else if (width > 500 && width < 1300) {
                        google_ad_slot = "1050010217";
                        google_ad_width = 468;
                        google_ad_height = 15;
                    }
                    else if (width > 1300) {
                        google_ad_slot = "9501564175";
                        google_ad_width = 728;
                        google_ad_height = 15;
                    }

                </script>
            </div><!--/span-->

            <div class="col-md-3 col-sm-3">
            {% block colonneDroite %}

            {% endblock %}
            </div><!--/span-->
        </div><!--/row-->

        <div class="row">
            <div class="panel-footer">
            {% block footer %}
            {% endblock %}
                <div class="container">
                    <!-- AddThis Follow BEGIN -->
                    <div class="addthis_toolbox addthis_32x32_style addthis_default_style">
                        <a class="addthis_button_facebook_follow" addthis:userid="androiddev.fr"></a>
                        <a class="addthis_button_twitter_follow" addthis:userid="androiddevfr"></a>
                        <a class="addthis_button_google_follow" addthis:userid="+Android-devFr"></a>
                        <a class="addthis_button_rss_follow" addthis:userid="http://www.android-dev.local/feeds/derniersArticlesAstuces.rss"></a>
                    </div>
                    <!-- AddThis Follow END -->

                    <p class="muted">Autres pages : <a href="{{ path('androiddev_contact') }}">Contact</a> - <a href="{{ path('androiddev_apropos') }}">A Propos</a> - <a href="{{ path('androiddev_mentions') }}">Mentions légales</a>  </p>
                    <p class="muted credit">&copy; Android-dev.fr / Propulsé par Symfony2 et Bootstrap | COPYRIGHT 2010 - 2014, tout droits réservés.</p>
                </div>
            </div>
        </div>


        <!-- Le javascript
    ================================================== -->
        <!-- Placed at the end of the document so the pages load faster -->
        <script type="text/javascript" src="{{ asset('js/jquery-1.8.3.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/javascript.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/jquery.fancybox.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/jquery.fancybox.pack.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/shCore.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/shBrushXml.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/shBrushJava.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/shBrushBash.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/isotope.pkgd.min.js') }}"></script>
        <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-4e84c63d79df556f"></script>
{% block script %}
{% endblock %}
        <script type="text/javascript">
            SyntaxHighlighter.config.tagName = "textarea";
            SyntaxHighlighter.all();
        </script>
    </body>
</html>