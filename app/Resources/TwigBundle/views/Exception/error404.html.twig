    <span class="icon-bar"></span>
</a>
<a class="brand" href="{{ path('androiddev_accueil') }}">{{ infosite['titre'] }}</a>
<div class="nav-collapse collapse">
    <ul class="nav" role="navigation">
        <li><a href="{{ path('androiddev_accueil') }}">Accueil</a></li>
        <li class="dropdown">
            <a data-toggle="dropdown" class="dropdown-toggle">Article <b class="caret"></b></a>
            <ul class="dropdown-menu">
                <li class="nav-header"><a href="{{ path('androiddev_article')}}">Tous les articles</a></li>

                    {% for categorie in filtrecat['article'] %}
                <li><a href="{{ path('androiddev_articleCat', {'slug': categorie.slug}) }}">{{categorie.getNom()}}</a></li>
                    {% endfor %}
                </ul>
            </li>
            <li class="dropdown">
                <a data-toggle="dropdown" class="dropdown-toggle">Astuce <b class="caret"></b></a>
                <ul class="dropdown-menu">
                    <li class="nav-header"><a href="{{ path('androiddev_astuce')}}">Toutes les astuces</a></li>

                    {% for categorie in filtrecat['astuce'] %}
                    <li><a href="{{ path('androiddev_astuceCat', {'slug': categorie.slug}) }}">{{categorie.getNom()}}</a></li>
                    {% endfor %}
                    </ul>
                </li>
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle">Projet <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="nav-header"><a href="{{ path('androiddev_projet')}}">Tous les projets</a></li>
        {% for projet in filtrecat['projet'] %}
                            <li><a href="{{ path('androiddev_projetVoir', {'slug': projet.slug}) }}">{{projet.getTitre()}}</a></li>
        {% endfor %}
                            </ul>
                        </li>
                    </ul>
                    <form action="{{ path('androiddev_search') }}" method="post" class="navbar-search pull-right">
                        <input type="text" name="motCles" class="search-query" placeholder="Rechercher sur android-dev.fr">
                    </form>
                </div><!--/.nav-collapse -->
{% endblock %}

