{% extends "AndroidDevAdminBundle::layout.html.twig" %}

{% block title %}Voir - {{ entity.titre }} - Android-dev{% endblock %}
{% block description %}{% endblock %}

{% block contentDetail %}
<div class="col-md-12">
    <h1>Projet</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Titre</th>
            <td>{{ entity.titre }}</td>
        </tr>
        <tr>
            <th>Soustitre</th>
            <td>{{ entity.sousTitre }}</td>
        </tr>
        <tr>
            <th>Contenu</th>
            <td>{{ entity.contenu }}</td>
        </tr>
        <tr>
            <th>Visible</th>
            <td>{{ entity.visible }}</td>
        </tr>
        <tr>
            <th>Contenufin</th>
            <td>{{ entity.contenuFin }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_projet') }}">
            Retour Ã  la liste
        </a>
    </li>
    <li>
        <a href="{{ path('admin_projet_edit', { 'id': entity.id }) }}">
            Modifier
        </a>
    </li>
    <li>
        <form action="{{ path('admin_projet_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Supprimer</button>
        </form>
    </li>
</ul>

    </div>
{% endblock %}