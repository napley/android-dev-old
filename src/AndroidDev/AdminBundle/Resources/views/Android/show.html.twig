{% extends "AndroidDevAdminBundle::layout.html.twig" %}

{% block title %}Voir - {{ entity.titre }} - Android-dev{% endblock %}
{% block description %}{% endblock %}


{% block contentDetail %}
<div class="col-md-12"><h1>Android</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ entity.titre }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_android') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('admin_android_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>
            <form action="{{ path('admin_android_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
                <button type="submit">Delete</button>
            </form>
        </li>
    </ul>


</div>
{% endblock %}