{% extends "AndroidDevAdminBundle::layout.html.twig" %}

{% block title %}Voir - {{ entity.username }} - Android-dev{% endblock %}
{% block description %}{% endblock %}

{% block contentDetail %}
    <div class="col-md-12"><h1>Utilisateur</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ entity.nom }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ entity.prenom }}</td>
        </tr>
        <tr>
            <th>Lienprofil</th>
            <td>{{ entity.lienProfil }}</td>
        </tr>
        <tr>
            <th>Textpresentation</th>
            <td>{{ entity.textPresentation }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_user') }}">
            Retour Ã  la liste
        </a>
    </li>
    <li>
        <a href="{{ path('admin_user_edit', { 'id': entity.id }) }}">
            Modifier
        </a>
    </li>
    <li>
        <form action="{{ path('admin_user_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Supprimer</button>
        </form>
    </li>
</ul>


            </div>

{% endblock %}