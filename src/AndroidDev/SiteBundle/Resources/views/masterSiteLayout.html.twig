{% block colonneDroite %}

    <div class="follow-button">
        <!-- AddThis Follow BEGIN -->
        <div class="addthis_toolbox addthis_32x32_style addthis_default_style">
            <a class="addthis_button_facebook_follow" addthis:userid="androiddev.fr"></a>
            <a class="addthis_button_twitter_follow" addthis:userid="androiddevfr"></a>
            <a class="addthis_button_google_follow" addthis:userid="+Android-devFr"></a>
            <a class="addthis_button_rss_follow" addthis:userid="http://www.android-dev.fr/feeds/derniersArticlesAstuces.rss"></a>
        </div>
        <!-- AddThis Follow END -->
    </div>

    {% if topArticle.titre is defined %}
        <div class="listeDroite">
            <ul class="nav nav-pills  nav-stacked">
                <li class="nav-header li-title-right"><span class="title-right gras">A la une</span></li>
                <li class="list-right">
                    {% if topArticle.vignette %}
                        <a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'Top']);" class="link-center" title="{{ topArticle.titre | raw }}" href="{{ path('androiddev_voir', {'slug': topArticle.slug}) }}"><img class="img-responsive larg-80" alt="{{ topArticle.titre | raw }}" src="{{ topArticle.vignette }}"/></a>
                    {% endif %}
                    <div class="topArticle">
                        <span class="title-article-center"><h2 class="title-list-article"><a title="{{ topArticle.titre | raw }}" href="{{ path('androiddev_voir', {'slug': topArticle.slug}) }}">{{ topArticle.titre | raw }}</a></h2></span>
                        <div class="pres-article-list">{{ topArticle.sousTitre | raw }}</div>
                    </div>
                </li>
            </ul>
        </div>
    {% endif %}

    <div class="listeDroite">
        <ul class="nav nav-pills  nav-stacked">
            <li class="nav-header li-title-right"><span class="title-right gras">Outils</span></li>
            <li class="list-right"><a href="{{ path('androiddev_fragmentation') }}"><img class="mini-vign-liste" src="/upload/images/vignette/statistique.jpg"/>Historique de la fragmentation android</a></li>
        </ul>
    </div>

    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- responsive 3 -->
    <ins class="adsbygoogle"
         style="display:block"
         data-ad-client="ca-pub-4672745701309961"
         data-ad-slot="8326182171"
         data-ad-format="auto"></ins>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>

    <div class="listeDroite">
        <ul class="nav nav-pills  nav-stacked">
            <li class="nav-header li-title-right"><span class="title-right gras">Top des 7 derniers jours</span></li>
                {% for stat in topPiwik %}
                <li class="list-right"><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'Stat']);" href="{{ path('androiddev_voir', {'slug': stat.Article.slug}) }}"><img class="mini-vign-liste" src="{% if stat.Article.vignette != '' %}{{ stat.Article.vignette }}{% else %}/img/no-img.jpg{% endif %}"/>{{ stat.Article.titre }}</a></li>
                {% endfor %}
        </ul>
    </div>

    <iframe src="http://rcm-eu.amazon-adsystem.com/e/cm?t=androiddevfr-21&o=8&p=12&l=st1&mode=books-fr&search=android%20programmation&fc1=000000&lt1=_blank&lc1=3366FF&bg1=FFFFFF&npa=1&f=ifr" marginwidth="0" marginheight="0" width="300" height="250" border="0" frameborder="0" style="border:none;" scrolling="no"></iframe>

    <div class="listeDroite">
        <ul class="nav nav-pills  nav-stacked">
            <li class="nav-header li-title-right"><span class="title-right gras">Mise Ã  jour</span></li>
                {% for article in lastUpdate %}
                <li class="list-right"><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'Update']);" href="{{ path('androiddev_voir', {'slug': article.slug}) }}"><img class="mini-vign-liste" src="{% if article.vignette != '' %}{{ article.vignette }}{% else %}/img/no-img.jpg{% endif %}"/>{{ article.titre | raw }}</a></li>
                {% endfor %}
        </ul>
    </div>

    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <!-- responsive 2 -->
    <ins class="adsbygoogle"
         style="display:block"
