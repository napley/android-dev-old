{% extends "::layout.html.twig" %}

{% block title %}Android-dev{% endblock %}
{% block description %}{% endblock %}
{% block urlCanonical %}{% endblock %}

{% block stylesheets %}
    {% stylesheets output="css/compiled/main.css" filter="cssrewrite" 
    'css/bootstrap.min.css'
    'css/bootstrap-theme.min.css'
    'css/style.css'
    'css/jquery.fancybox.css'
    'css/shCore.css'
    'css/shThemeDefault.css' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    {% block meta %}
    {% endblock %}
{% endblock %}

{% block navbar %}

    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">android-dev.fr</a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div style="height: 1px;" class="navbar-collapse collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav">
                <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavHome']);" href="{{ path('androiddev_accueil') }}">Accueil</a></li>
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle">Article <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="nav-header title-nav"><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavArticleAll']);" href="{{ path('androiddev_article')}}">Tous les articles</a></li>

                        {% for categorie in filtrecat['article'] %}
                            <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavArticle']);" href="{{ path('androiddev_articleCat', {'slug': categorie.slug}) }}">{{categorie.getNom()}}</a></li>
                            {% endfor %}
                    </ul>
                </li>
                <li class="dropdown">
                    <a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavAstuceAll']);" data-toggle="dropdown" class="dropdown-toggle">Astuce <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="nav-header title-nav"><a href="{{ path('androiddev_astuce')}}">Toutes les astuces</a></li>

                        {% for categorie in filtrecat['astuce'] %}
                            <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavAstuce']);" href="{{ path('androiddev_astuceCat', {'slug': categorie.slug}) }}">{{categorie.getNom()}}</a></li>
                            {% endfor %}
                    </ul>
                </li>
                <li class="dropdown">
                    <a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavProjetAll']);" data-toggle="dropdown" class="dropdown-toggle">Projet <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="nav-header title-nav"><a href="{{ path('androiddev_projet')}}">Tous les projets</a></li>
                            {% for projet in filtrecat['projet'] %}
                            <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'NavProjet']);" href="{{ path('androiddev_projetVoir', {'slug': projet.slug}) }}">{{projet.getTitre()}}</a></li>
                            {% endfor %}
                    </ul>
                </li>
            </ul>

            <form action="{{ path('androiddev_search') }}" method="post" class="navbar-form navbar-right" role="search">
                <div class="form-group">
                    <input type="text" title="Rechercher sur android-dev.fr" name="motCles" class="form-control" placeholder="Rechercher sur android-dev.fr">
                </div>
            </form>


        </div><!-- /.navbar-collapse -->
    </nav>

{% endblock %}

