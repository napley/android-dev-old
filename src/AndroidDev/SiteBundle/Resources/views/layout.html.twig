{% block menuGauche %}    
<div class="title-left">
    <h3>
        Tous les <a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'AllArticle']);" class="title-list-left" href="{{ path('androiddev_article')}}">Articles</a>
    </h3>
    <span class="blue-border"></span>
</div>
<ul class="list-left">
        {% for categorie in filtrecat['article'] %}
    <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'TypeArticle']);" href="{{ path('androiddev_articleCat', {'slug': categorie.slug}) }}">{{categorie.getNom()}}</a></li>
        {% endfor %}
</ul>

<div class="title-left">
    <h3>
        Toutes les <a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'AllAstuce']);" class="title-list-left" href="{{ path('androiddev_astuce')}}">Astuces</a>
    </h3>
    <span class="blue-border"></span>
</div>
<ul class="list-left">
        {% for categorie in filtrecat['astuce'] %}
    <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'TypeAstuce']);" href="{{ path('androiddev_astuceCat', {'slug': categorie.slug}) }}">{{categorie.getNom()}}</a></li>
        {% endfor %}
</ul>

<div class="title-left">
    <h3>
        Tous les <a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'AllProjet']);" class="title-list-left" href="{{ path('androiddev_projet')}}">Projets</a>
    </h3>
    <span class="blue-border"></span>
</div>
<ul class="list-left">
        {% for projet in filtrecat['projet'] %}
    <li><a onclick="javascript:_paq.push(['trackEvent', 'Clic', 'TypeProjet']);" href="{{ path('androiddev_projetVoir', {'slug': projet.slug}) }}">{{projet.getTitre()}}</a></li>
        {% endfor %}
</ul>
